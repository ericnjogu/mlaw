<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="contract_assignment_maintenance_view" parent="martinlaw_shared_assignment_maintenance_view">
		<property name="headerText" value="Contract Assignment Maintenance" />
		<property name="dataObjectClassName" value="org.martinlaw.bo.contract.Assignment" />
		<property name="items">
			<list merge="true">
				<bean parent="martinlaw_shared_assignment_maintenance_view_assignees">
					<property name="collectionObjectClass"
						value="org.martinlaw.bo.contract.Assignee" />
				</bean>
			</list>
		</property>
	</bean>

</beans>