<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="conveyance_consideration_inquiry_view" parent="martinlaw_shared_consideration_inquiry_view">
		<property name="headerText" value="Conveyance Consideration Inquiry" />
		<property name="dataObjectClassName" value="org.martinlaw.bo.conveyance.Consideration" />
		<property name="items">
			<list merge="true">
				<bean parent="martinlaw_shared_consideration_transactions_sub_collection_inquiry">
					<property name="collectionObjectClass" value="org.martinlaw.bo.conveyance.TransactionDoc" />
				</bean>
			</list>
		</property>
	</bean>

</beans>