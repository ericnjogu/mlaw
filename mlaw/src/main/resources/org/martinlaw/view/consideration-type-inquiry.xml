<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="consideration_type_inquiry_view" parent="martinlaw_shared_name_description_inquiry_view">
    <property name="headerText" value="Consideration Type Inquiry"/>
    <property name="dataObjectClassName" value="org.martinlaw.bo.ConsiderationType"/>
    <property name="items">
		<list merge="true">
			<!-- <ref bean="martinlaw_shared_name_description_inquiry_view_items"/> -->
			<bean parent="martinlaw_shared_scope_collection_inquiry_section" >
				<property name="collectionObjectClass" value="org.martinlaw.bo.ConsiderationTypeScope" />
			</bean>
		</list>
	</property>
  </bean>
</beans>
