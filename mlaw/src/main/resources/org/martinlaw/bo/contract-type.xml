<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Copyright 2012 Protosoft Ltd -->
	
	<bean id="contract_type_bo" parent="BusinessObjectEntry">
	<property name="businessObjectClass" value="org.martinlaw.bo.ContractType"/>
		<property name="titleAttribute" value="name" />
		<property name="attributes">
			<list>
				<ref bean="contract_type_bo_id" />
				<ref bean="contract_type_bo_name" />
				<ref bean="contract_type_bo_description" />
			</list>
		</property>
		<property name="objectLabel" value="Contract Type" />
	</bean>

	<!-- Attribute Definitions -->

	<bean id="contract_type_bo_description" parent="martinlaw-shared-description" />

	<bean id="contract_type_bo_id" parent="martinlaw-shared-id" />

	<bean id="contract_type_bo_name" parent="martinlaw-shared-name" />
</beans>
