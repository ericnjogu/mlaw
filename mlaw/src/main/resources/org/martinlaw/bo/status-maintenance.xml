<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="case-statusMaintDocument" parent="case-statusMaintDocument-parentBean"/>

  <bean id="case-statusMaintDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.martinlaw.bo.Status"/>
    <property name="maintainableClass" value="org.kuali.rice.krad.maintenance.MaintainableImpl"/>
    <property name="lockingKeys">
      <list>
        <value>status</value>
      </list>
    </property>
    <property name="documentTypeName" value="StatusMaintenanceDocument"/>
  </bean>

</beans>
