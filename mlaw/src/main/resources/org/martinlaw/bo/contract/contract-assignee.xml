  <bean id="contract_assignee_bo" parent="martinlaw_shared_person_parentBean">
    <property name="businessObjectClass" value="org.martinlaw.bo.contract.ContractAssignee"/>
    <property name="titleAttribute" value="principalName"/>
    <property name="attributes">
      <list>
        <ref bean="contract_assignee_bo-principal-name"/>
      </list>
    </property>
    <property name="objectLabel" value="Assignee"/>

  </bean>

<!-- Attribute Definitions -->
 
 <bean id="contract_assignee_bo-principal-name" parent="martinlaw-shared-principal-name" p:label="Assignee"/>

</beans>
