<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         
http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- Copyright 2010-2011 Protosoft Ltd -->
	
	<bean id="contract_signatory_bo" parent="contract_signatory_bo_parentBean" />

	<bean id="contract_signatory_bo_parentBean" abstract="true"
		parent="martinlaw_shared_person_parentBean">
		<property name="businessObjectClass" value="org.martinlaw.bo.contract.ContractSignatory" />
		<property name="titleAttribute" value="principalName" />
		<property name="attributes">
			<list>
				<ref bean="contract_signatory_bo_principal_name" />
			</list>
		</property>
		<property name="objectLabel" value="Signatory" />
	</bean>

	<!-- Attribute Definitions -->
	<bean id="contract_signatory_bo_principal_name" parent="martinlaw-shared-principal-name"
		p:label="Signatory Name" />

</beans>
