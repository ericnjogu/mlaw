-->

  <bean id="contractMaintDocument" parent="contractMaintDocument-parentBean"/>

  <bean id="contractMaintDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.martinlaw.bo.contract.Contract"/>
    <property name="maintainableClass" value="org.kuali.rice.krad.maintenance.MaintainableImpl"/>
    <!-- does this cause only one maint document to be enroute at a time - Yes subject to a unique combination of the keys below -->
    <property name="lockingKeys">
      <list>
        <value>localReference</value>
      </list>
    </property>

    <property name="documentTypeName" value="ContractMaintenanceDocument"/>
  </bean> 
</beans>
