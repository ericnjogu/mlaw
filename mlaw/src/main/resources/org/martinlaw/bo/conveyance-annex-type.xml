<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         
http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<!-- Copyright 2012 Protosoft Ltd -->
	<bean id="conveyance_annex_type" parent="conveyance_annex_type-parentBean" />

	<bean id="conveyance_annex_type-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.martinlaw.bo.ConveyanceAnnexType" />

		<property name="titleAttribute" value="name" />
		<property name="attributes">
			<list>
				<ref bean="conveyance_annex_type_description" />
				<ref bean="conveyance_annex_type_name" />
				<ref bean="conveyance_annex_type_id" />
			</list>
		</property>
		<property name="objectLabel" value="Conveyance Annex Type" />
	</bean>

	<!-- Attribute Definitions -->

	<bean id="conveyance_annex_type_description" parent="martinlaw-shared-description" />

	<bean id="conveyance_annex_type_name" parent="martinlaw-shared-name" p:label="Conveyance Annex Type"/>

	<bean id="conveyance_annex_type_id" parent="martinlaw-shared-id" />

</beans>
