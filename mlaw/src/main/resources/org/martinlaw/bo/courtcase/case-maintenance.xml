  <bean id="caseMaintDocument" parent="caseMaintDocument-parentBean"/>

  <bean id="caseMaintDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
    <property name="dataObjectClass" value="org.martinlaw.bo.courtcase.CourtCase"/>
    <property name="maintainableClass" value="org.kuali.rice.krad.maintenance.MaintainableImpl"/>
    <!-- does this cause only one maint document to be enroute at a time - Yes subject to a unique combination of the keys below -->
    <property name="lockingKeys">
      <list>
      	<value>name</value>
        <value>localReference</value>
      </list>
    </property>

    <property name="documentTypeName" value="CaseMaintenanceDocument"/>
  </bean> 
</beans>
